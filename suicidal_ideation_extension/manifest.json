{
    "manifest_version": 2,
    "name": "Suicidal Ideation Detection",
    "version": "1.0.0",
    "description": "A web extension to detect suicidal ideation in social media text.",
    "icons": {
      "16": "icons/icon16.png"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png"
      }
    },
    "manifest_key": "value"
  }
  